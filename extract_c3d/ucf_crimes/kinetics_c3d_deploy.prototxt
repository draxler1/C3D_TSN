name: "C3D"
mem_param {
  optimize_test: true
  optimize_train: true
}
input: "data"
input_shape {
  dim: 10
  dim: 3
  dim: 16
  dim: 112
  dim: 112
}
#######################################################
layer {
  name: "conv1"
  type: "Convolution"
  bottom: "data"
